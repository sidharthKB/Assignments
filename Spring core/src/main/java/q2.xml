<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
          <bean id="ans" class="maven_demo.Question">
          <property name="questionId" value="2"></property>
          <property name="question" value="How many states are there in India"></property>
          <property name="answers">
          <list>
          <value>Twenty Nine</value>
          <value>Thirty</value>
          </list>
          </property>
          <property name="answers1">
          <set>
          <value>thirty one</value>
          <value>twenty</value>
          </set>
          </property>
          <property name="answers2">
          <map>
          <entry key="1" value="twentyfive"></entry>
          <entry key="2" value="thirtytwo"></entry>
          </map>
          </property>         
         
          
          
          </bean>
           </beans>